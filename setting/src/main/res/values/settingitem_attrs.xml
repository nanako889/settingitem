<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalSettingItem">
        <attr name="hsi_left_image" format="reference" />
        <attr name="hsi_title" format="string" />
        <attr name="hsi_subtitle" format="string" />
        <attr name="hsi_right_image" format="reference" />
        <attr name="hsi_show_right_image" format="boolean" />
        <attr name="hsi_show_right_text" format="boolean" />
    </declare-styleable>

    <declare-styleable name="VerticalSettingItem">
        <attr name="vsi_top_image" format="reference" />
        <attr name="vsi_title" format="string" />
        <attr name="vsi_title_size" format="dimension"/>
        <attr name="vsi_title_color" format="color"/>
        <attr name="vsi_nv_margin_left" format="dimension"/>
        <attr name="vsi_nv_margin_bottom" format="dimension"/>
        <attr name="vsi_title_margin_top" format="dimension"/>
        <attr name="vsi_top_image_size" format="dimension"/>
    </declare-styleable>

</resources>