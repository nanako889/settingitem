<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".MainActivity"
    >

    <com.qbw.settingitem.HorizontalSettingItem
        android:id="@+id/hsi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:hsi_title="标题标题"
        app:hsi_subtitle="子标题子标题"
        app:hsi_left_image="@drawable/vip"
        app:hsi_right_image="@drawable/arrow"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <com.qbw.settingitem.VerticalSettingItem
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/hsi"
        app:layout_constraintRight_toRightOf="parent"
        app:vsi_top_image="@drawable/pay"
        app:vsi_title="标题"
        app:layout_constraintLeft_toLeftOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>